<% @body_id = 'fourohfour-page' %>
<%= content_for :page do %>
<section class="content interior grid">
  <% unless (@page.content_for :body).empty? %>
  <div class="intro clearfix">
    <div class="-inner">
      <header class="section-title">
        <h1 data-text="<%=raw @page.title %>"><%=raw @page.title %></h1>
        <div class="summary user-content">
        <%= raw @page.content_for :body -%>
        </div>
      </header>
    </div>
  </div>
  <% end %>
</section>
<% end -%>
