<div class="secondary one-half -right">
  <div class="secondary-item">
    <header>
      <h1>Highlights</h1>
    </header>
    <div class="summary highlights">
      <table cellpadding="0" cellspacing="0">
        <thead class="visually-hidden">
          <tr>
          <th class="date"><h3>Date</h3></th><th><h3>Details</h3></th>
          <tr>
        </thead>
        <tbody>
        <% @highlights.each do |item| %>
          <tr>
            <td class="date"><%= item[0] -%></td>
            <td><%= raw item[1] -%></td>
          </tr>
        <% end -%>
        </tbody>
      </table>
    </div>
  </div>
</div>