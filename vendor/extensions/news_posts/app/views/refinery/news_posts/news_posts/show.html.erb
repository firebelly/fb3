<% content_for :title, @page_title %>

<% content_for :meta do %>
  <link rel="alternate" type="application/rss+xml" title="Firebelly News" href="<%= refinery.news_posts_news_posts_url(format: 'rss') %>"> 
<% end -%>

<%= content_for :aside do %>
  <%= render :partial => "refinery/news_posts/news_posts/nav" -%>
<% end -%>

<% content_for :page do %>
  <%= render :partial => "shared/news_post", :locals => { :news_post => @news_post, :is_single => true } %>
<% end %>
